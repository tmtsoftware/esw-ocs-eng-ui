{
  "name": "esw-ocs-eng-ui",
  "author": "TMT",
  "version": "0.6.0",
  "license": "Apache License, Version 2.0",
  "type": "module",
  "dependencies": {
    "@ant-design/pro-layout": "^7.22.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@tmtsoftware/esw-ts": "github:tmtsoftware/esw-ts#86554f7",
    "antd": "^5.23.4",
    "globals": "^15.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-router-dom": "^7.1.5"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:reporter": "tsc --p tsconfig.test.json",
    "test:unit": "web-test-runner 'test/**/*.unit.test.{ts,tsx}'",
    "test:unit:watch": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit -- --watch",
    "xxx1": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner test/containers/obsversation/SelectedObsMode.unit.test.tsx --watch",
    "xxx2": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner test/features/sequencer/components/sequencerDetails/StepListTable.unit.test.tsx --watch",
    "xxx3": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner test/routes/Routes.unit.test.tsx --watch",
    "xxx4": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner test/components/HeaderBar/HeaderBar.unit.test.tsx --watch",
    "test:reporter": "npm run test:unit -- --config wtr-rtm.config.js --coverage",
    "test:ci": "npm-run-all build:reporter test:reporter",
    "test": "npm run test:unit",
    "test:logsEnabled": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean eslint:check style:check",
    "eslint:check": "eslint --config eslint.config.mjs './{src,test}/**/*.{ts,tsx}'",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier style:check",
    "fix:eslint": "eslint --fix --config eslint.config.mjs './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' ",
    "audit": "npm audit",
    "style:check": "stylelint 'src/**/*.css' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@johanblumenberg/ts-mockito": "^1.0.45",
    "@remcovaes/web-test-runner-vite-plugin": "^1.2.2",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@web/dev-server-import-maps": "^0.2.1",
    "@web/test-runner": "^0.19.0",
    "chai": "^5.1.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "keycloak-js": "^25.0.6",
    "less": "^4.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0"
  }
}
