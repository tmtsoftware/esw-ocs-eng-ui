{
  "name": "esw-ocs-eng-ui",
  "author": "TMT",
  "version": "0.4.0",
  "license": "Apache License, Version 2.0",
  "type": "module",
  "dependencies": {
    "@tmtsoftware/esw-ts": "github:tmtsoftware/esw-ts#b1bbe53",
    "antd": "^4.16.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.34.12",
    "react-router-dom": "^6.2.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:reporter": "tsc --p tsconfig.test.json",
    "test:unit": "web-test-runner 'test/**/*.unit.test.{ts,tsx}'",
    "test:unit:watch": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit -- --watch",
    "test:reporter": "npm run test:unit -- --config wtr-rtm.config.js --coverage",
    "test:ci": "npm-run-all build:reporter test:reporter",
    "test": "npm run test:unit",
    "test:logsEnabled": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean eslint:check style:check",
    "eslint:check": "eslint --config .eslintrc.json './{src,test}/**/*.{ts,tsx}'",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier style:check",
    "fix:eslint": "eslint --fix --config .eslintrc.json './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' ",
    "audit": "npm audit",
    "style:check": "stylelint 'src/**/*.css' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/chai": "^4.3.7",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@web/dev-server-import-maps": "^0.0.6",
    "@web/test-runner": "0.13.26",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "keycloak-js": "^22.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "@typestrong/ts-mockito": "^2.6.6",
    "typescript": "^4.9.5",
    "vite": "^2.7.13",
    "vite-web-test-runner-plugin": "^0.1.0"
  }
}
