{
  "name": "esw-ocs-eng-ui",
  "author": "TMT",
  "version": "0.1.0-RC2",
  "license": "Apache License, Version 2.0",
  "type": "module",
  "dependencies": {
    "@tmtsoftware/esw-ts": "0.2.0",
    "antd": "^4.16.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-query": "^3.27.0",
    "react-router-dom": "^5.3.0"
  },
  "scripts": {
    "start": "vite serve",
    "build": "vite build",
    "build:reporter": "tsc --p tsconfig.test.json",
    "test:unit": "web-test-runner 'test/**/*.unit.test.{ts,tsx}'",
    "test:unit:watch": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit -- --watch",
    "test:reporter": "npm run test:unit -- --config wtr-rtm.config.js --coverage",
    "test:ci": "npm-run-all build:reporter test:reporter",
    "test": "npm run test:unit",
    "test:logsEnabled": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean eslint:check style:check audit",
    "eslint:check": "eslint --config .eslintrc.json './{src,test}/**/*.{ts,tsx}'",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier style:check",
    "fix:eslint": "eslint --fix --config .eslintrc.json './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' ",
    "audit": "npm audit",
    "style:check": "stylelint 'src/**/*.css' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/chai": "^4.2.22",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@web/dev-server-import-maps": "^0.0.6",
    "@web/test-runner": "0.13.20",
    "chai": "^4.3.4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.0.0-alpha.10",
    "keycloak-js": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.10",
    "vite-web-test-runner-plugin": "^0.1.0"
  }
}
