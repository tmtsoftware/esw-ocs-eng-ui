{
  "name": "esw-ocs-eng-ui",
  "author": "TMT",
  "version": "0.4.0",
  "license": "Apache License, Version 2.0",
  "type": "module",
  "dependencies": {
    "@tmtsoftware/esw-ts": "github:tmtsoftware/esw-ts#ca67cd5",
    "antd": "^4.24.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "@tanstack/react-query": "^5.24.1",
    "@tanstack/react-query-devtools": "^5.24.1",
    "react-router-dom": "^6.22.2"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:reporter": "tsc --p tsconfig.test.json",
    "test:unit": "web-test-runner 'test/**/*.unit.test.{ts,tsx}'",
    "test:x": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/actions/LoadSequence.unit.test.tsx'",
    "test:x2": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/actions/StopSequence.unit.test.tsx'",
    "test:x3": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/sequencerDetails/DeleteAction.unit.test.tsx'",
    "test:x4": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/sequencerDetails/ParameterTable.unit.test.tsx'",
    "test:x5": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/sequencerDetails/ReplaceStep.unit.test.tsx'",
    "test:x6": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/sequencerDetails/SequencerDetails.unit.test.tsx'",
    "test:x7": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/sequencerDetails/StepListTable.unit.test.tsx'",
    "test:x8": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/agent/components/SpawnSequenceComponent.unit.test.tsx'",
    "test:x9": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/agent/components/AgentCards.unit.test.tsx'",
    "test:x10": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/routes/Routes.unit.test.tsx'",
    "test:x11": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/containers/obsversation/Observations.unit.test.tsx'",
    "test:x12": "export ESW_OCS_ENG_UI_LOGS=true && web-test-runner 'test/features/sequencer/components/**/*.unit.test.tsx'",
    "test:unit:watch": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit -- --watch",
    "test:reporter": "npm run test:unit -- --config wtr-rtm.config.js --coverage",
    "test:ci": "npm-run-all build:reporter test:reporter",
    "test": "npm run test:unit",
    "test:logsEnabled": "export ESW_OCS_ENG_UI_LOGS=true && npm run test:unit",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean eslint:check style:check",
    "eslint:check": "eslint --config .eslintrc.json './{src,test}/**/*.{ts,tsx}'",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier style:check",
    "fix:eslint": "eslint --fix --config .eslintrc.json './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' ",
    "audit": "npm audit",
    "style:check": "stylelint 'src/**/*.css' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@remcovaes/web-test-runner-vite-plugin": "^1.2.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/chai": "^4.3.12",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@typestrong/ts-mockito": "^2.6.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@web/dev-server-import-maps": "^0.2.0",
    "@web/test-runner": "^0.18.0",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "keycloak-js": "^23.0.7",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}
